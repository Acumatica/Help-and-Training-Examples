							"cookie": [],
							"body": "{\n    \"id\": \"330470e7-7381-e511-80c0-00155d012302\",\n    \"rowNumber\": 1,\n    \"note\": \"\",\n    \"OrderNbr\": {\n        \"value\": \"000006\"\n    },\n    \"OrderType\": {\n        \"value\": \"SO\"\n    },\n    \"custom\": {\n        \"Document\": {\n            \"AttributeOPERATSYST\": {\n                \"type\": \"CustomStringField\",\n                \"value\": \"Windows 10\"\n            }\n        }\n    },\n    \"files\": []\n}"
						}
